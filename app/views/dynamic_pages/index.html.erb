<div class = 'hero'>
</div>
	<div class="herotext col-xs-10 col-xs-offset-1">
		<div class="h2text">
			Like that Pokemon game 
			<br /> but without the fucking Pokemon
			<br /> and for trees 
		</div>
		<%= link_to 'Play Now', new_admin_course_path, class: 'btn-lg btn-success' %>
	</div>
	<div class="row">
		<% @latest_trees.each do |tree| %>
			<div class="grid-home col-xs-10 col-xs-offset-1 col-md-offset-0 col-md-4">
					<%= tree.title %>
					<div class="center-block">
						<a class="thumbnail">
							<%= image_tag tree.image, class: 'img-responsive' %>
							<%= link_to 'Name This Tree', adventure_path(tree), class: 'btn-lg btn-success' %>
							<br/>
							<br/>
						</a>
					</div>
			</div>
		<% end %>
	</div>
